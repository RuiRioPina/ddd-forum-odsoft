{
	"tags": {
		"allowUnknownTags": true,
		"dictionaries": ["jsdoc","closure"]
	},
	"plugins": ["plugins/markdown","node_modules/better-docs/typescript"],
	"recurseDepth": 10,
	"source": {
		"include": ["src"],
		"includePattern": ".ts$|.js$",
		"excludePattern": "(node_modules/|docs)"
		},
	"templates": {
		"cleverLinks": true,
		"monospaceLinks": true
	},
	"opts": {
		"destination": "./jsdoc",
		"recurse": true,
		"readme": "./readme.md"
	}
}